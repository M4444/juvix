[
    ( "open" "Prelude" )
,
    ( "open" "LLVM" )
,
    ( "type"
        ( "bar" ":type" "ty" ) ()
        ( "P" "int" "int" )
        ( "::" "int" "int" ) )
,
    ( "declare" "infixl" "::" 10 )
,
    ( ":defsig" "pattern-match" ( ":infix" "->" "bar" "int" ) )
,
    ( ":defun-match" "pattern-match"
        ( ( ( "P" "x" "y" ) ) ( ":infix" "Int.+" "x" "y" ) )
        ( ( ( ":infix" "::" "x" "y" ) ) ( ":infix" "Int.+" "x" "y" ) ) )
,
    ( ":defsig" "main" "int" )
,
    ( ":defun-match" "main" ( () 3 ) ) ]