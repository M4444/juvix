mod Norm where

open Prelude
open LLVM

type SomePoint = P3 int int int | P2 int int 

sig norm : SomePoint -> double
let norm (P3 x y z) = sqrt (x^2 + y^2 + z^2)
let norm (P2 x y) = sqrt (x^2 + y^2)

